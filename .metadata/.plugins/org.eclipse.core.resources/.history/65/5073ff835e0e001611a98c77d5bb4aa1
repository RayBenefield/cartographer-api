package com.cartographerapi.domain;

import java.util.List;

/**
 * Writer repository interface for Objects. This provides access to a
 * data source that contains the Object.
 * 
 * @author GodlyPerfection
 *
 */
public interface DomainObjectReader {
	
	/**
	 * Get multiple domain Objects from a single segment page.
	 * 
	 * @param objects
	 * @return
	 */
	public List<Object> getDomainObjects(SegmentScannerRequest objects);

}
